void repairTheStreet()
{
    repeat (9)
    {
        StrassenRepe();
    }
    
}


void StrassenRepe()
{
    if (! rightIsClear())
    {
        moveForward();
    }else
    {
        turnRight();
        moveForward();
        dropBeeper();
        turnAround();
        moveForward();
        turnRight();
        moveForward();
    } 
}
